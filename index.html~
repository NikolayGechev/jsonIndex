$.getJSON("http://10.5.0.182/jsonPath/store.json", function(json) {

    var store2_2 =  new JSONIndex( json, [ {index: "$..name"}, {index: "$..style"}, { left_join: "$.shirt[?(@.owner=={{$.person..id}})].[style, color]" } ] );

});

